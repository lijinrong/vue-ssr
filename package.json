{
    "name": "webserver",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "serve": "vue-cli-service serve",
        "dev": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:client": "vue-cli-service build",
        "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build --mode server",
        "build:win": "npm run build:server && mv dist/vue-ssr-server-bundle.json bundle && npm run build:client && mv bundle/vue-ssr-server-bundle.json dist/vue-ssr-server-bundle.json"
    },
    "dependencies": {
        "autoprefixer": "^9.7.6",
        "axios": "^0.19.2",
        "cookie-parser": "~1.4.4",
        "core-js": "^3.3.2",
        "css-loader": "^3.5.2",
        "debug": "~2.6.9",
        "express": "~4.17.3",
        "http-errors": "~1.6.3",
        "log4js": "^6.2.0",
        "memory-fs": "^0.5.0",
        "morgan": "~1.9.1",
        "postcss-loader": "^3.0.0",
        "postcss-px2rem": "^0.3.0",
        "qs": "^6.9.0",
        "style-loader": "^1.1.4",
        "webpack-node-externals": "^1.7.2"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-plugin-eslint": "^4.0.0",
        "@vue/cli-service": "^4.0.0",
        "babel-eslint": "^10.0.3",
        "cross-env": "^7.0.2",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "less": "^3.11.1",
        "less-loader": "^5.0.0",
        "lodash.merge": "^4.6.2",
        "node-sass": "^4.13.0",
        "sass-loader": "^8.0.0",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vue-server-renderer": "^2.6.11",
        "vue-template-compiler": "^2.6.10",
        "webpack-cli": "^3.3.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
